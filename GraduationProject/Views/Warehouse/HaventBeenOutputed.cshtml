@model List<GraduationProject.ViewModels.WareHouse.haventbeenoutputed>
<form method="post">
    <label>  أدخل التاريخ الابتدائي لمعرفة المواد التي لم يتم تخريجها  </label>
    <input type="datetime-local" name="period" />
    <br />
    <div class="table-responsive">
        <table class="styled-table">
            <thead>
                <tr>
                    <th>الطلب</th>
                    <th>المواد</th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    for (int i = 0; i < Model.Count(); i++)
                    {
                        <tr>
                            <td>@Model[i].Order.User.RequstingParty</td>
                        </tr>
                    }
                }
                else
                {
                    <tr>
                        <td>@ViewBag.Empty </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
    <input type="submit" value="yalla" />
</form>
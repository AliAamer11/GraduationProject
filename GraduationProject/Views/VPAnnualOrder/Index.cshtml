@model IEnumerable<GraduationProject.Data.Models.AnnualOrder>


@if (Model.Count() > 0)
{

    <table>
        <thead>
            <tr>
                <td>كمية الفصل الاول </td>
                <td>كمية الفصل الثاني  </td>
                <td>كمية الفصل الصيفي</td>
                <td>كمية الفصل الصيفي</td>
                <td>كمية الفصل الصيفي</td>
                <td>معدل التدفق </td>
                <td>المعدل التقريبي</td>
                <td>تعليق</td>
            </tr>

        </thead>
        @foreach (var obj in Model)
        {

            <tbody>
                <tr>
                    <td>@obj.FirstSemQuantity</td>
                    <td>@obj.SecondSemQuantity</td>
                    <td>@obj.ThirdSemQuantity</td>
                    <td>@obj.FlowRate</td>
                    <td>@obj.ApproxRate</td>
                    <td>@obj.Comment</td>

                </tr>
            </tbody>
        }
    </table>
}
else
{ <h6>لا يوجد طلبات</h6>}
<form method="post" asp-action="ApproveOrder" asp-controller="VPAnnualOrder" asp-route-id=@Model.First().OrderId>
    <button type="submit">الموافقة </button>
</form>


